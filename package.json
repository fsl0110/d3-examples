{
  "name": "atomic-charts",
  "version": "0.0.1",
  "description": "D3 Chart Library using Atomic Design and React composition pattern",
  "author": "Frank Simon Lechner",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsl0110/atomic-charts.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:serve": "yarn build && serve ./build",
    "serve": "serve ./build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "styleguide": "yarn styleguidist server",
    "styleguide:build": "yarn styleguidist build",
    "styleguide:build:serve": "yarn styleguidist build && serve ./styleguide",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "tslint -p ./tsconfig.json",
    "lint:styles": "stylelint src/**/styles.tsx",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/moment": "^2.13.0",
    "@types/styled-components": "^4.1.18",
    "axios": "^0.19.0",
    "babel-loader": "8.0.5",
    "classnames": "^2.2.6",
    "d3": "^5.9.7",
    "immer": "^3.1.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize-scss": "^7.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-spring": "^8.0.27",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/d3": "^5.7.2",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/moxios": "^0.4.8",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "moxios": "^0.4.0",
    "node-sass": "^7.0.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript": "^1.12.5",
    "react-scripts": "3.0.1",
    "react-styleguidist": "^9.1.11",
    "serve": "^11.1.0",
    "source-map-explorer": "^2.0.1",
    "stylelint": "^10.1.0",
    "stylelint-order": "^3.0.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-scss": "^3.9.3",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "^0.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/examples/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/build/",
      "!<rootDir>/styleguide/",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
